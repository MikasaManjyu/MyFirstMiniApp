{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/farcaster.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/.well-known/farcaster.json",
      "destination": "/farcaster.json"
    }
  ]
}
